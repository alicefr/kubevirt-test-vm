[global]
ioengine=libaio
invalidate=1
ramp_time=5
iodepth=1
runtime=20
time_based
direct=1

[write-device-to-test-4k-seq]
stonewall
bs=4k
filename=/dev/device-to-test
rw=write
write_bw_log=fio-4k-device-to-test-write-seq.results
write_iops_log=fio-4k-device-to-test-write-seq.results
[read-device-to-test-4k-seq]
stonewall
bs=4k
filename=/dev/device-to-test
rw=read
write_bw_log=fio-4k-device-to-test-read-seq.results
write_iops_log=fio-4k-device-to-test-read-seq.results
[randread-device-to-test-4k-seq]
stonewall
bs=4k
filename=/dev/device-to-test
rw=randread
write_bw_log=fio-4k-device-to-test-randread-seq.results
write_iops_log=fio-4k-device-to-test-randread-seq.results
[randwrite-device-to-test-4k-seq]
stonewall
bs=4k
filename=/dev/device-to-test
rw=randwrite
write_bw_log=fio-4k-device-to-test-randwrite-seq.results
write_iops_log=fio-4k-device-to-test-randwrite-seq.results
[write-device-to-test-1m-seq]
stonewall
bs=1m
filename=/dev/device-to-test
rw=write
write_bw_log=fio-1m-device-to-test-write-seq.results
write_iops_log=fio-1m-device-to-test-write-seq.results
[read-device-to-test-1m-seq]
