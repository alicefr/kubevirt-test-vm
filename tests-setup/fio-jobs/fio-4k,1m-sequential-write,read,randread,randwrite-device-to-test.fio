bs=1m
filename=/dev/device-to-test
rw=read
write_bw_log=fio-1m-device-to-test-read-seq.results
write_iops_log=fio-1m-device-to-test-read-seq.results
[randread-device-to-test-1m-seq]
stonewall
bs=1m
filename=/dev/device-to-test
rw=randread
write_bw_log=fio-1m-device-to-test-randread-seq.results
write_iops_log=fio-1m-device-to-test-randread-seq.results
